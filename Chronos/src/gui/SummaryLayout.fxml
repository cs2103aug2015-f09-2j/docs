<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="summaryTable" editable="true" fixedCellSize="0.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="IDCol" prefWidth="25.0" text="ID" />
          <TableColumn fx:id="timeCol" prefWidth="100.0" text="time" />
          <TableColumn fx:id="titleCol" prefWidth="340.0" text="title" />
          <TableColumn fx:id="categoryCol" prefWidth="70.0" text="category" />
          <TableColumn fx:id="noteCol" prefWidth="45.0" text="note" />
        </columns>
      </TableView>
   </children>
</fx:root>
